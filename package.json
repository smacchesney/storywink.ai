{
  "name": "storywink-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:worker": "tsc --project tsconfig.worker.json --listEmittedFiles",
    "start": "next start",
    "lint": "next lint --fix",
    "format": "prettier --write .",
    "check-types": "tsc --noEmit",
    "check-format": "prettier --check .",
    "check-lint": "next lint",
    "check-all": "npm run check-types && npm run check-format && npm run check-lint",
    "db:start": "docker compose up -d",
    "db:stop": "docker compose down",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "worker:dev": "nodemon --watch src/queues --watch src/lib --ext ts --exec \"npm run build:worker && node dist-worker/queues/workers/story-generation.worker.js\"",
    "dev:all": "concurrently \"npm:dev\" \"npm:worker:dev\""
  },
  "dependencies": {
    "@clerk/nextjs": "^6.16.0",
    "@dnd-kit/core": "^6.3.1",
    "@prisma/client": "^6.6.0",
    "@radix-ui/react-alert-dialog": "^1.1.7",
    "@radix-ui/react-aspect-ratio": "^1.1.3",
    "@radix-ui/react-dropdown-menu": "^2.1.7",
    "@radix-ui/react-label": "^2.1.3",
    "@radix-ui/react-radio-group": "^1.2.4",
    "@radix-ui/react-select": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-switch": "^1.1.4",
    "@radix-ui/react-tooltip": "^1.2.0",
    "bullmq": "^5.48.1",
    "class-variance-authority": "^0.7.1",
    "cloudinary": "^2.6.0",
    "clsx": "^2.1.1",
    "dotenv": "^16.5.0",
    "ioredis": "^5.6.1",
    "lucide-react": "^0.488.0",
    "next": "15.3.0",
    "next-themes": "^0.4.6",
    "openai": "^4.93.0",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-resizable-panels": "^2.1.7",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.2.0",
    "tw-animate-css": "^1.2.5",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.1.2",
    "eslint": "^9",
    "eslint-config-next": "15.3.0",
    "eslint-config-prettier": "^10.1.2",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "prisma": "^6.6.0",
    "tailwindcss": "^4",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5"
  },
  "_moduleAliases": {
    "@": "./dist-worker"
  }
}
